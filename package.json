{
	"name": "amazon-scraper",
	"version": "1.0.0",
	"description": "Amazon product scraper with Bun backend and Vite frontend",
	"main": "backend/index.js",
	"scripts": {
		"dev": "concurrently \"bun run backend:dev\" \"bun run frontend:dev\"",
		"backend:dev": "cd backend && bun run dev",
		"frontend:dev": "cd frontend && bun run dev",
		"build": "cd frontend && bun run build",
		"test": "bun test",
		"start": "cd backend && bun run start"
	},
	"keywords": [
		"amazon",
		"scraper",
		"bun",
		"vite",
		"web-scraping"
	],
	"author": "Amazon Scraper Team",
	"license": "MIT",
	"devDependencies": {
		"concurrently": "^8.2.2"
	}
}